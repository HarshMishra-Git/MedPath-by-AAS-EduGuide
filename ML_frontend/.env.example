# NEET College Finder - Environment Variables Example
# Copy this file to .env and configure your values

# API Configuration
VITE_ML_API_URL=http://localhost:8000
VITE_NEET_API_URL=http://localhost:8001
VITE_API_TIMEOUT=30000

# Application Settings
VITE_APP_NAME="NEET ML Predictor"
VITE_APP_VERSION="2.0.0"
VITE_APP_DESCRIPTION="AI-Powered NEET College Finder with Expert Recommendations"

# NEET College Finder Backend Configuration
# Alternative way to configure backend (components are combined to build URL)
VITE_NEET_BACKEND_HOST=localhost
VITE_NEET_BACKEND_PORT=8001
VITE_NEET_BACKEND_PROTOCOL=http

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PDF_EXPORT=true
VITE_ENABLE_NEET_FINDER=true

# API Endpoints (relative paths - usually no need to change)
VITE_HEALTH_ENDPOINT=/health
VITE_SEARCH_ENDPOINT=/search
VITE_EXPORT_PDF_ENDPOINT=/export-pdf
VITE_STATES_ENDPOINT=/states
VITE_QUOTAS_ENDPOINT=/quotas
VITE_CATEGORIES_ENDPOINT=/categories
VITE_COURSES_ENDPOINT=/courses

# External Services (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_analytics_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# Production Configuration Example:
# VITE_NEET_API_URL=https://api.neetcollegefinder.com
# VITE_NEET_BACKEND_HOST=api.neetcollegefinder.com
# VITE_NEET_BACKEND_PORT=443
# VITE_NEET_BACKEND_PROTOCOL=https
# VITE_ENABLE_DEBUG=false